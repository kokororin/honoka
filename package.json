{
  "name": "honoka",
  "version": "0.4.9",
  "description": "Just a fetch() API wrapper for both Browser and Node.js.",
  "main": "index.js",
  "browser": "lib/honoka.js",
  "unpkg": "lib/honoka.min.js",
  "typings": "index.d.ts",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --progress",
    "lint": "eslint ./src",
    "start": "webpack --watch",
    "test": "cross-env NODE_ENV=test karma start && npm run lint",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kokororin/honoka.git"
  },
  "keywords": [
    "fetch",
    "xhr",
    "ajax",
    "promise",
    "lovelive"
  ],
  "author": "kokororin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kokororin/honoka/issues"
  },
  "homepage": "https://github.com/kokororin/honoka#readme",
  "dependencies": {
    "node-fetch": "^1.7.2",
    "url-search-params-polyfill": "^2.0.1",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.18.2",
    "chai": "^4.1.1",
    "clean-webpack-plugin": "^0.1.17",
    "coveralls": "^2.13.1",
    "cross-env": "^5.0.5",
    "eslint": "^4.4.1",
    "eslint-config-kotori": "^0.1.3",
    "istanbul-instrumenter-loader": "^3.0.0",
    "karma": "^1.7.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-express-http-server": "^0.0.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.4",
    "mocha": "^3.5.0",
    "multer": "^1.3.0",
    "puppeteer": "^0.12.0",
    "system-sleep": "^1.3.6",
    "webpack": "^3.5.3"
  },
  "files": [
    "lib",
    "index.js",
    "index.d.ts"
  ]
}
